#include <stdio.h>
 int main()
  {
      printf("Enter any number from 1 to 5 to choose what you want to eat: ");
      int food=0;
      scanf("%d",&food);

   switch (food)
    { case 1: printf("Food Item - Pasta\n Rs 179\n");
    break;
     case 2: printf("Food Item - Burger\n Rs 129\n");
    break;
      case 3: printf("Food Item - Sandwich\n Rs 149\n");
    break;
      case 4: printf("Food Item - Pizza\n Rs 239\n");
    break;
      case 5:printf("Food Item - French Fries\n Rs 99\n");
      break;

        default: printf("Choose between 1 to 5");
    }
     return 0;
   }
